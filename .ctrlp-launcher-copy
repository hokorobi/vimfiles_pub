append >	call AppendQuote('>')
substitute \ to /	call Substitute('\', '/')
substitute - to _	call Substitute('-', '_')
substitute _ to -	call Substitute('_', '-')
substitute X to Y	call SubstituteInput()
copy Today	call TodayCopy()
hosts	call ReadfileCopy('C:\WINDOWS\System32\drivers\etc\hosts')
